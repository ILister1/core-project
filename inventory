[manager]
swarm-manager


[workers]
swarm-worker


[manager:vars]
ansible_user=user 
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
ansible_python_interpreter=/usr/bin/python3

[workers:vars]
ansible_user=user 
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
ansible_python_interpreter=/usr/bin/python3